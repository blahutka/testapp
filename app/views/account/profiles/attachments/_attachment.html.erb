<li id="<%= dom_id(attachment) %>">
  <a href="#">
    <label for="<%= dom_id(attachment) %>_check" style="padding:0;float:left;width:auto;text-align: left;">
      <%= image_tag(attachment.file_url(:thumb)) %>
      <% if local_assigns[:f] %>
          <%= f.check_box(:_destroy, :checked => false, :id => "#{dom_id(attachment)}_check") %>

      <% else %>
          <span class="label success">Nov√Ω</span>
      <% end %>

      <%= attachment.file_identifier %>
    </label>
  </a>
</li>
