<div class="row">
  <div class="page-header">
    <h1>Dashboard</h1>
  </div>
</div>

<div class="row">
  <h2>Job Requests</h2>
  <table>
    <% @skill_requests.each do |skill_request| %>
        <tr>
          <td><%= link_to skill_request.title, skill_request_path(skill_request),
                          :id => dom_id(skill_request) %>
          </td>
        </tr>
    <% end %>
  </table>
</div>